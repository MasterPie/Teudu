<UserControl x:Class="Teudu.InfoDisplay.BoardNavigatorControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:teudu="clr-namespace:Teudu.InfoDisplay"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel Orientation="Horizontal">
        <teudu:EventBoard x:Name="PreviousBoard"/>
        <teudu:EventBoard x:Name="CurrentBoard"/>
        <teudu:EventBoard x:Name="NextBoard"/>
    </StackPanel>
</UserControl>
